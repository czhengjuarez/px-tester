{
  "name": "px-tester-worker",
  "version": "1.0.0",
  "description": "Cloudflare Workers API for PX Tester",
  "main": "src/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create px-tester-db",
    "db:migrate": "wrangler d1 execute px-tester-db --file=./schema.sql",
    "db:seed": "wrangler d1 execute px-tester-db --file=./seed.sql",
    "db:setup": "npm run db:migrate && npm run db:seed"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "d1"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@cloudflare/puppeteer": "^1.0.4"
  }
}
